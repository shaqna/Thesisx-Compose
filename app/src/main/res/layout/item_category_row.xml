<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cardCategory"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/range_6"
    android:clickable="true"
    app:cardCornerRadius="100dp">

    <TextView
        android:id="@+id/tv_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/range_32"
        android:layout_marginTop="@dimen/range_16"
        android:layout_marginRight="@dimen/range_32"
        android:layout_marginBottom="@dimen/range_16"
        android:fontFamily="@font/inter_semibold"
        android:textColor="@color/white"
        tools:text="Kendali" />
</androidx.cardview.widget.CardView>